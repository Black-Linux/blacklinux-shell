VAR => (

DISTRO.NAME;        "BlackLinux"
VERSION;            1.4
DISTRO.ID;          ubuntu
NAME;               "BlackLinux"
LTS;                true

)

SC => (
    if (LTS = true); then
        var.lts = "LTS"

    @output (
        add ("DISTRO.NAME", "VERSION", "var.lts")
        @var (
            o = add
        )
    )
)

OUT ( FUNC.SC => "o")
